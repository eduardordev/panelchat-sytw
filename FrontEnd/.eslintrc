{
    "parser": "babel-eslint",
    "rules":{
        "no-console":"warn",
        "linebreak-style":"off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/no-unresolved": "off",
        "react/no-typos": "off",
	"jsx-a11y/click-events-have-key-events": "off",
        "jsx-a11y/no-static-element-interactions": "off"
    },
    "plugins": ["react", "import", "jsx-a11y"],
    "extends": ["airbnb"],
    "env": {
        "browser": true,
        "node": true
    }
}